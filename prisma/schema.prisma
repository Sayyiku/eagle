generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./eagleuse.db?connection_limit=1"
}

model Image {
  id               String   @id
  name             String
  size             Int
  btime            BigInt
  mtime            BigInt
  ext              String
  tags             String
  folders          String
  url              String
  annotation       String
  modificationTime BigInt
  width            Int
  height           Int
  palettes         String
  lastModified     BigInt
  isDeleted        Boolean?
  deletedTime      BigInt?
}
